<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="@color/awui_ripple_light" xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/awui_blue1_ligth"/>
            <corners android:radius="10dp" />
        </shape>
    </item>

    <item android:left="2dp" android:right="2dp" android:top="2dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/awui_blue1_ligth"></solid>
            <corners android:radius="10dp"></corners>
            <stroke android:width="1dp" android:color="@color/awui_blue1"></stroke>
        </shape>
    </item>
</ripple>
